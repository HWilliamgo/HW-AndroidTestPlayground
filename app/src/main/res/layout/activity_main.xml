<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_jump_to_file_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="file test" />

        <Button
            android:id="@+id/btn_show_toast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/btn_join"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="加入频道" />

        <Button
            android:id="@+id/btn_leave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="离开频道" />

        <Button
            android:id="@+id/btn_animate_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:text="开始动画" />

        <Button
            android:id="@+id/btn_jump_to_second_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="跳转到SecondActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_third_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="跳转到ThirdActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_fired_lock_screen_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到FiredLockScreenActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_view_test_Activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到ViewTestActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_coroutine_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:text="跳转到协程Activity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_stack_test_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到栈测试Activity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_connectivity_test_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到ConnectivityTestActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_jump_to_html_text_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到html text activity" />

        <Button
            android:id="@+id/btn_jump_to_fragment_dialog_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到FragmentDialogActivity" />

        <Button
            android:id="@+id/btn_jump_to_rv_test_Activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转到RvTestActivity" />

        <Button
            android:id="@+id/btn_jump_to_rx_java_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试RxJava" />

        <Button
            android:id="@+id/btn_jump_to_web_view_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="webView测试" />

        <Button
            android:id="@+id/btn_jump_to_multi_process_webview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="多进程WebView测试">

        </Button>

        <Button
            android:id="@+id/btn_jump_to_video_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="video view">

        </Button>

        <Button
            android:id="@+id/btn_jump_to_image_load"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/btn_jump_to_progress_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/btn_jump_to_jniTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/btn_jump_to_https_sni"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="https_sni" />

        <Button
            android:id="@+id/btn_jumpt_to_plv_test"
            android:onClick="onClick"
            android:text="跳转到PLV测试"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/btn_jump_to_empty_module"
            android:onClick="onClick"
            android:text="空模块测试"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>


</ScrollView>